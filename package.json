{
  "name": "openai",
  "homepage": "./",
  "main": "./public/electron.js",
  "description": "NuTool - Code Generator",
  "author": "Nuvoton Technology Corp.",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@reduxjs/toolkit": "^2.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.68",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "antd": "^5.12.3",
    "langchain": "^0.0.210",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rimraf package-lock.json & rimraf node_modules",
    "cleanb": "rimraf build",
    "electron:start": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron:package:mac": "npm run build && electron-builder -m",
    "electron:package:win": "npm run build && electron-builder -w",
    "electron:package:linux": "npm run build && electron-builder -l",
    "electron:clean": "rimraf dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electronmon": "^2.0.2",
    "rimraf": "^5.0.5",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.nuvoton.codegenerator",
    "productName": "NuTool - Code Generator",
    "extends": null,
    "asar": false,
    "npmRebuild": false,
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": [
        "zip"
      ],
      "icon": "build/NuTool-CodeGenerator_B-256.ico"
    },
    "linux": {
      "target": "deb"
    }
  }
}
